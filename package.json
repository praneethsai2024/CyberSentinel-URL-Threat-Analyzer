{
  "name": "cybersentinel",
  "version": "1.0.0",
  "description": "AI-Powered URL Threat Analyzer with Random Forest Algorithm - Advanced cybersecurity tool for real-time URL threat detection and analysis",
  "private": true,
  "homepage": "./",
  "main": "src/index.js",
  "dependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^5.0.1",
    "recharts": "^3.1.2",
    "web-vitals": "^5.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,css,md}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "cybersecurity",
    "url-analysis",
    "threat-detection",
    "machine-learning",
    "random-forest",
    "security",
    "malware-detection",
    "phishing-detection",
    "ai-security",
    "web-security",
    "react",
    "javascript",
    "frontend"
  ],
  "author": {
    "name": "CyberSentinel Team",
    "email": "contact@cybersentinel.com",
    "url": "https://cybersentinel.com"
  },
  "contributors": [
    {
      "name": "AI Security Developer",
      "email": "dev@cybersentinel.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cybersentinel.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cybersentinel/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.6.2",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
